import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		//getting input
		Scanner sc =new Scanner(System.in);
		int d=sc.nextInt();
		int s=sc.nextInt();
		int t=sc.nextInt();
		if(d<0||s<0||t<0){
		    System.out.println("Invalid Input");
		}
		else if((d/s)<t){
		    System.out.println("Yes");
		}
		else{
		    System.out.println("No");
		}
	}
}
